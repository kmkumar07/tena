{
  "name": "tenant-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --source-map --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run tenant-portal:storybook",
    "build-storybook": "ng run tenant-portal:build-storybook",
    "buildcompodoc": "compodoc -p tsconfig.json",
    "compodoc": "npx compodoc -s tsconfig.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.2.9",
    "@angular/material-moment-adapter": "^16.1.1",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@types/moment": "^2.13.0",
    "json2typescript": "^1.5.1",
    "ngx-tippy-wrapper": "^6.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@babel/core": "^7.21.8",
    "@angular/localize": "^15.2.5",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-docs": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addon-mdx-gfm": "^7.0.5",
    "@storybook/angular": "^7.0.5",
    "@storybook/testing-library": "^0.1.0",
    "@types/jasmine": "~4.3.0",
    "babel-loader": "^8.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.8",
    "typescript": "~4.8.2"
  },
  "stylePreprocessorOptions": {
    "includePaths": [
      "node_modules",
      "src/assets/styles"
    ]
  }
}
